{
  "name": "wadify/wadify-sdk-php",
  "description": "Wadify SDK for PHP - Use Wadify Web Services in your PHP project",
  "keywords": ["wadify","sdk"],
  "type": "library",
  "authors": [
    {
      "name": "Wadify",
      "email": "hello@wadify.com"
    }
  ],
  "support": {
    "issues": "https://github.com/wadify/wadify-sdk-php/issues"
  },
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.6",
    "guzzlehttp/guzzle": "^6.2",
    "symfony/dependency-injection": "^3.0",
    "symfony/config": "^3.0",
    "symfony/yaml": "^3.0",
    "sainsburys/guzzle-oauth2-plugin": "~3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.3",
    "mikey179/vfsStream": "^1.6"
  },
  "autoload": {
    "psr-4": {
      "Wadify\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wadify\\Test\\": "tests/"
    }
  }
}
